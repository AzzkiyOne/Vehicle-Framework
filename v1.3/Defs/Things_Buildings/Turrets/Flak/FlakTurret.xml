		<terrainAffordanceNeeded>Heavy</terrainAffordanceNeeded>
		<building>
			<turretGunDef>Turret_Flak_Weapon</turretGunDef>
			<turretTopDrawSize>4.5</turretTopDrawSize>
			<turretTopOffset>(0, 1)</turretTopOffset>
			<buildingTags>
				<li>Artillery_AirDefense</li>
			</buildingTags>
			<turretBurstWarmupTime>1</turretBurstWarmupTime>
			<turretBurstCooldownTime>3.25</turretBurstCooldownTime>
