<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<ThingDef ParentName="BaseArtilleryBuilding">
		<defName>Turret_Flak</defName>
		<label>Flak Cannon</label>
		<description>A manned artillery that launches all kinds of shells. Capable of inflicting devastating damage to both enemy forces and structures, this military machinery excels at laying down fire support for soldiers in the field.\n\nCapable of targeting other areas of the world within a medium radius.</description>
		<thingClass>Vehicles.Building_RecoiledTurret</thingClass>
		<graphicData>
			<texPath>Things_Building/TurretFlak_Base</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<drawSize>(3,3)</drawSize>
			<damageData>
				<rect>(0.38,0.2,0.56375,0.8)</rect>
				<cornerTL>Damage/Corner</cornerTL>
			</damageData>
			<shadowData>
				<offset>(-0.13,-0.87,-0.1)</offset>
				<volume>(0.5,0.4,1.05)</volume>
			</shadowData>
		</graphicData>
		<statBases>
			<MaxHitPoints>250</MaxHitPoints>
			<WorkToBuild>3600</WorkToBuild>
		</statBases>
		<techLevel>Industrial</techLevel>
		<interactionCellOffset>(0,0,-2)</interactionCellOffset>
		<hasInteractionCell>true</hasInteractionCell>
		<size>(3,3)</size>
		<rotatable>true</rotatable>
		<stealable>false</stealable>
		<stuffCategories>
			<li>Metallic</li>
		</stuffCategories>
		<costStuffCount>100</costStuffCount>
		<costList>
			<Steel>180</Steel>
			<ComponentIndustrial>9</ComponentIndustrial>
		</costList>
		<comps>
			<li Class="CompProperties_Explosive">
				<explosiveRadius>5.9</explosiveRadius>
				<explosiveDamageType>Bomb</explosiveDamageType>
			</li>
			<li Class="Vehicles.CompProperties_DrawLayerTurret"> 
				<graphicDatas> 
					<li> 
						<texPath>Things_Building/TurretFlak_Body</texPath> 
						<graphicClass>Graphic_Single</graphicClass> 
						<shaderType>CutoutComplex</shaderType>
						<drawSize>(4.5,4.5)</drawSize>
						<drawOffset>(0,0.001,0)</drawOffset> 
						<damageData> 
							<rect>(0.38,0.2,0.56375,0.8)</rect> 
							<cornerTL>Damage/Corner</cornerTL> 
						</damageData> 
						<shadowData> 
							<offset>(-0.13,-0.87,-0.1)</offset> 
							<volume>(0.5,0.4,1.05)</volume> 
						</shadowData> 
					</li> 
					<li> 
						<texPath>Things_Building/TurretFlak_Rail</texPath> 
						<graphicClass>Graphic_Single</graphicClass>
						<shaderType>CutoutComplex</shaderType> 
						<drawSize>(4.5,4.5)</drawSize>
						<drawOffset>(0,-0.001,0)</drawOffset> 
						<damageData> 
							<rect>(0.38,0.2,0.56375,0.8)</rect> 
							<cornerTL>Damage/Corner</cornerTL> 
						</damageData> 
						<shadowData> 
							<offset>(-0.13,-0.87,-0.1)</offset> 
							<volume>(0.5,0.4,1.05)</volume> 
						</shadowData> 
					</li> 
